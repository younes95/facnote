{% extends 'base.html.twig' %}

{% block body %}
	<table id="utilisateurTable" class="table table-striped table-bordered" style="width:100%">
		<thead>
			<tr>
				<th scope="col">Raison sociale</th>
				<th scope="col">Ville</th>
				<th scope="col">SIRET</th>
			</tr>
		</thead>
		<tbody>
	    {% for societe in societes %}

	    	{% if ((societe.isSocietePrincipale == 1) and (societe.idTypeSociete.id == 1)) %}
	    	{% else %}
			<tr>
				<td onclick="RemplirtableauAjax('{{ societe.tokenSociete}}', 'societe');">
					<div class="input-group-prepend">				
							{{ render(controller('App\\Controller\\DroitController::aDroitTwig', {idUtilisateur: idUtilisateurdroit, idSociete: societe.id, controllerId: controllerIddroit, tokenSociete: societe.tokenSociete, RaisonSocial: societe.RaisonSocial, })) }}
					</div>
					
				</td>
				<td onclick="RemplirtableauAjax('{{ societe.tokenSociete}}', 'societe');">{{ societe.Ville}}</td>
				<td onclick="RemplirtableauAjax('{{ societe.tokenSociete}}', 'societe');">{{ societe.SIRETSociete }}</td>

			</tr>
			{% endif %}
		{%endfor%}
		</tbody>
		
	</table>
	
		
{% endblock %}